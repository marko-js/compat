{
  "name": "marko-widgets",
  "version": "8.0.11",
  "homepage": "https://github.com/marko-js/compat/blob/main/README.md",
  "bugs": "https://github.com/marko-js/compat/issues/new?template=Bug_report.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/marko-js/compat"
  },
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    "./marko.json": "./marko.json",
    "./renderer": "./renderer.js",
    "./dom": "./dom.js",
    ".": "./index.js",
    "./*.marko": "./*.marko",
    "./*.d.ts": "./*.d.ts",
    "./*.json": "./*.json",
    "./*": {
      "import": "./*.mjs",
      "default": "./*.js"
    }
  },
  "files": [
    "dist",
    "marko.json",
    "dom.js",
    "index.js",
    "renderer.js",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@marko/babel-utils": "^6.3.1",
    "@marko/compat-utils": "^1.0.1",
    "@marko/compat-v4": "^1.0.7",
    "raptor-dom": "^1.1.1",
    "raptor-renderer": "^1.5.0"
  },
  "peerDependencies": {
    "@marko/compiler": "^5",
    "marko": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
