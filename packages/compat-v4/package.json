{
  "name": "@marko/compat-v4",
  "version": "1.0.7",
  "homepage": "https://github.com/marko-js/compat/blob/main/README.md",
  "bugs": "https://github.com/marko-js/compat/issues/new?template=Bug_report.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/marko-js/compat"
  },
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    "./marko.json": "./marko.json",
    "./*.marko": "./*.marko",
    "./*.d.ts": "./*.d.ts",
    "./*.json": "./*.json",
    "./*": {
      "import": "./*.mjs",
      "default": "./*.js"
    }
  },
  "files": [
    "dist",
    "marko.json",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@marko/babel-utils": "^6.3.1",
    "@marko/compat-utils": "^1.0.1"
  },
  "peerDependencies": {
    "@marko/compiler": "^5",
    "marko": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
